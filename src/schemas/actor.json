{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "format": "uuid"
        },
        "display_name": {
            "type": "string"
        },
        "aliases": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "notes": {
            "type": "string"
        },
        "created_at": {
            "type": "string",
            "format": "date-time"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time"
        }
    },
    "required": [
        "id",
        "display_name",
        "created_at",
        "updated_at"
    ],
    "additionalProperties": false
}